# Gemini API Configuration
# Get your API key from: https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Specify the Gemini model to use
# Default: gemini-2.5-flash-lite (recommended for speed and cost)
# Options: gemini-2.5-flash-lite, gemini-2.5-flash, gemini-2.0-flash
VITE_GEMINI_MODEL=gemini-2.5-flash-lite

# Parsing Configuration
# Timeout for parsing operations (milliseconds)
VITE_PARSING_TIMEOUT_MS=30000

# Maximum retry attempts if parsing fails
VITE_PARSING_MAX_RETRIES=3

# Enable debug logging in console
VITE_ENABLE_DEBUG_LOGGING=false

# Optional: Enable/disable AI features
VITE_ENABLE_AI_PARSING=true
